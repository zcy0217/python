   "source": [
    "請製作一個可以倒轉數字的程式~\n",
    "\n",
    "    輸入:\n",
    "    135486351\n",
    "    輸出:\n",
    "    153684531\n",
    "    請不要用轉文字方式寫"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      ":135486351\n",
      "153684531\n"
     ]
    }
   ],
   "source": [
    "x = int(input(\":\"))\n",
    "reversedNum =0\n",
    "while(x>0):\n",
    "    reversedNum = reversedNum * 10 + x % 10\n",
    "    x = x // 10\n",
    "print(reversedNum)"
   ]
  }
