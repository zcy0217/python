      "source": [
        "num = int(input())\n",
        "\n",
        "print(num, '= ', end = '')\n",
        "\n",
        "for i in range(2, num + 1) :\n",
        "  while True :\n",
        "    if num % i == 0 :\n",
        "      print(i, end = '')\n",
        "      num = num / i\n",
        "      if num == 1 :\n",
        "        break\n",
        "      else :\n",
        "        print('*', end = '')\n",
        "    else :\n",
        "      break"
      ],
