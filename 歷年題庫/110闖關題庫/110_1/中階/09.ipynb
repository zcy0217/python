        "a, b = map(int, input().split())\n",
        "\n",
        "for i in range(b) :\n",
        "  if i == 0 or i == (b - 1) :\n",
        "    length = '*' * a\n",
        "    print(\" \".join(length))\n",
        "  else :\n",
        "    height = \"**\"\n",
        "    space = ' ' * (2 * a - 3)\n",
        "    print(space.join(height))"
